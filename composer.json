{
  "name": "onsigbaar/components",
  "description": "library",
  "keywords": [
    "onsigbaar",
    "components"
  ],
  "homepage"    : "https://github.com/onsigbaar/components",
  "license"     : "MIT",
  "authors": [
    {
      "name"    : "anonymoussc",
      "email"   : "50c5ac69@opayq.com",
      "homepage": "https://github.com/onsigbaar",
      "role"    : "Developer"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.3",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^3.7",
    "friendsofphp/php-cs-fixer": "^2.7",
    "laravel/framework": "5.7.*",
    "spatie/phpunit-snapshot-assertions": "^1.0",
    "phpstan/phpstan": "^0.9.2"
  },
  "autoload": {
    "psr-4": {
      "Onsigbaar\\Components\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Onsigbaar\\Components\\Tests\\": "tests",
      "Modules\\Recipe\\": "tests/stubs/valid/Recipe"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Onsigbaar\\Components\\LaravelModulesServiceProvider"
      ],
      "aliases": {
        "Module": "Onsigbaar\\Components\\Facades\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
